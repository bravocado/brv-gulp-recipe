{
  "name": "nrd-gulp-recipe",
  "version": "0.1.0",
  "description": "Gulp starter with common tasks and scenarios",
  "repository": {
    "type": "git",
    "url": ""
  },
  "browser": {
    "underscore": "backbone/node_modules/underscore",
    "plugin": "./build/scripts/vendor/jquery-plugin.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "hbsfy"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": ">=1.3.6",
    "browserify-shim": "~3.7.0",
    "coffeeify": ">=0.7.0",
    "gulp": ">=3.8.7",
    "gulp-changed": ">=0.4.1",
    "gulp-imagemin": ">=1.1.0",
    "gulp-notify": ">=1.4.2",
    "gulp-ruby-sass": ">=0.7.1",
    "gulp-util": ">=3.0.0",
    "pretty-hrtime": ">=0.2.1",
    "require-dir": ">=0.1.0",
    "watchify": "~1.0.6",
    "del": ">=0.1.0",
    "gulp-jade": ">=0.8.0",
    "gulp-filter": ">=1.0.2",
    "gulp-data": "~1.0.2",
    "gulp-sass": "~1.0.0",
    "gulp-sourcemaps": "~1.2.2",
    "browserify": "~5.12.1",
    "vinyl-source-stream": "~1.0.0",
    "gulp-autoprefixer": "~1.0.1",
    "gulp-concat": "~2.4.1",
    "gulp-rename": "^1.2.0",
    "critical": "^0.2.0",
    "gulp-minify-css": "~0.3.10",
    "backbone": "~1.1.2",
    "handlebars": "~2.0.0",
    "hbsfy": "~2.2.0",
    "gulp-uglify": "~1.0.1",
    "gulp-streamify": "0.0.5"
  },
  "dependencies": {
    "jquery": ">=1.11.0",
    "anila": ">=2.1.1"
  }
}